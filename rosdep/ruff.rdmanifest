uri: 'https://raw.githubusercontent.com/zygo-be/rosdep-manifest/main/rosdep/empty.tar'
md5sum: 578ad3648be7e297a751874684166651
install-script: |
  #!/usr/bin/env bash
  set -e
  echo "Installing ruff via pip3..."
  pip3 install ruff --break-system-packages
  echo "Ruff installation complete."
check-presence-script: |
  #!/usr/bin/env bash
  if command -v ruff >/dev/null 2>&1; then
    exit 0
  else
    echo "Ruff not installed"
    exit 1
  fi
